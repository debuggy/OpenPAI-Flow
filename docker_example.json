{
    "version": 0.1,
    "environment": {
      "base_docker": "pai.build.base:hadoop2.7.2-cuda9.0-cudnn7-devel-ubuntu16.04",
      "env_variables": {
          "TENSORFLOW_VERSION": "1.4.0",
          "LC_ALL": "C"
      },
      "components": {
          "python": {
              "version": "3.6"
          },
          "git": {
              "version": "latest"
          }
      },
      "python_requirements": {
          "tensorflow": {
              "version": "1.4.0",
              "is_gpu": true
          }
      }
    },
    "dataset": {
        "local": {
            "file_type": "directory",
            "path": "./data"
        }
    },
    "code": {
        "local": {
            "file_type": "directory",
            "path": ".",
            "exclude": "./data"
        }
    },
    "image_output": {
        "registry": "dockerhub"
    }
  }