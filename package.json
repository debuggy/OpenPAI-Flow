{
  "name": "gelato",
  "version": "0.2.3",
  "description": "A work flow for ai researcher to build training environment.",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "author": "debuggy",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).js?(x)"
    ]
  },
  "scripts": {
    "debug": "node ./lib/cli.js --output ./out/debug ./examples/config-example.yml",
    "test": "jest",
    "test-dev": "jest common/",
    "test-release": "jest release/",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "standard"
  },
  "files": [
    "lib/",
    "!lib/__tests__",
    "schemas/"
  ],
  "dependencies": {
    "ajv": "^6.5.5",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "execa": "^1.0.0",
    "jest": "^23.6.0",
    "standard": "^12.0.1"
  }
}
