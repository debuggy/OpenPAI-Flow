{
  "version": 0.1,
  "name": "flow_test",
  "author": "debuggy",
  "env_variables": {
    "TENSORFLOW_VERSION": "1.4.0",
    "LC_ALL": "C"
  },
  "base_docker": "pai.build.base:hadoop2.7.2-cuda9.0-cudnn7-devel-ubuntu16.04",
  "run_steps": [
    {
      "name": "install_conda",
      "type": "install_conda",
      "config": {
        "env_name": "flow_example",
        "python_version": "3.6"
      }
    },
    {
      "name": "install_tensorflow",
      "type": "conda_install",
      "config": {
        "packages": [
          {
            "name": "scipy",
            "version": "0.15.0"
          },


        ]
      }
    },
    {
      "name": "prepare_code",
      "type": "git_clone",
      "config": {
        "type": "github",
        "url": "https://github.com/debuggy/DockerForPAI_init.git",
        "branch": "master"
      }
    }
  ],
  "entrypoint_steps": [
    {
      "name": "custom_install",
      "type": "custom_command",
      "config": {
        "command": "python hello.py"
      }
    }
  ]
}
